msgid "EpubMerge"
msgstr "EpubMerge"

#: epubmerge_plugin.py:62
msgid "Merge multiple EPUBs into one in a new book."
msgstr "Mehrere ePubs zu einem neuen Buch zusammenfügen."

#: epubmerge_plugin.py:134
msgid "&Merge Epubs"
msgstr "ePubs &zusammenfügen"

#: epubmerge_plugin.py:138
msgid "&UnMerge Epub"
msgstr "ePubs &teilen"

#: epubmerge_plugin.py:146
msgid "&Configure Plugin"
msgstr "Plugin &konfigurieren"

#: epubmerge_plugin.py:148 epubmerge_plugin.py:149
