msgstr "Abbrechen"

#: dialogs.py:116
msgid "Starting Merge..."
msgstr "Beginne Zusammenfügen..."

#: dialogs.py:174
msgid "Move selected books up the list"
msgstr "Gewählte Bücher nach oben bewegen"

#: dialogs.py:179
msgid "Remove selected books from the list"
msgstr "Gewählte Bücher aus Liste löschen"

#: dialogs.py:184
msgid "Move selected books down the list"
msgstr "Gewählte Bücher nach unten bewegen"

#: dialogs.py:223
msgid "Author(s)"
msgstr "Autor(en)"

#: dialogs.py:223
msgid "Series"
msgstr "Serie"

#: dialogs.py:223
msgid "Title"
msgstr "Titel"

#: dialogs.py:272
msgid "Are you sure you want to remove this book from the list?"
msgstr "Wollen Sie dieses Buch wirklich aus der Liste löschen?"

#: dialogs.py:274
msgid "Are you sure you want to remove the selected %d books from the list?"
msgstr "Wollen Sie die gewählten %d Bücher wirklich aus der Liste löschen?"

